#include <iostream>
#include <string>
#include <algorithm>
#include <vector>


using namespace std;

int main() {

    int arr[26];
    string s1;

    cin >> s1;

    //배열 arr의 원소를 모두 -1로 변경
    fill_n(arr,26,-1);

    for(int i=0; i<s1.length(); i++){
        //만약 이미 한 번 count 했다면 넘어간다
        if(arr[s1[i] - 'a'] != -1)
            continue;
        //해당하는 자리에 index를 넣는다
        arr[s1[i]-'a'] = i;
    }

    for(int i=0; i<26; i++){
        cout << arr[i] << " ";
    }

    return 0;
}
