#include <iostream>
#include <algorithm>
#include <string>
#include <cstring>
#include <cmath>
#include <vector>
#include <queue>
#include <stack>
#include <unordered_set>

#define INF 987654321
using namespace std;

// 9613 - GCD í•©
int n;
vector<int> nums;


int getGCD(int a, int b){
    if(b == 0)  return a;
    return getGCD(b, a % b);
}

void sumOfGCD(){
    long long sum = 0;
    for(int i = 0; i < nums.size(); i++){
        for(int j = i + 1; j < nums.size(); j++){
            sum += getGCD(nums[i], nums[j]);
        }
    }
    cout << sum << '\n';
}

int main(){

    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    int testcase;
    cin >> testcase;
    while(testcase--){
        cin >> n;
        nums.assign(n, 0);
        for(int i = 0; i < n; i++){
            cin >> nums[i];
        }

        sumOfGCD();
    }

    return 0;
}
