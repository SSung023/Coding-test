#include <iostream>
#include <algorithm>
#include <string>
#include <cstring>
#include <cmath>
#include <vector>
#include <queue>
#include <map>
#include <unordered_map>

#define INF 987654321
#define MAX 1000000 + 1
using namespace std;

// 16916 - 부분 문자열
char s[MAX], p[MAX];


int main(){

    ios::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);

    cin >> s >> p;

    char *ptr = strstr(s, p);
    
    if(ptr == nullptr){
        cout << "0";
    }
    else{
        cout << "1";
    }

    return 0;
}
